class Solution {
    public int countSubstring(String s) {
        int[] freq = new int[26];
        for (char c : s.toCharArray()) {
            freq[c - 'a']++;
        }
        
        
        int result = 0;
        for (int f : freq) {
            result += f * (f + 1) / 2;
        }
        
        return result;
    }
}
