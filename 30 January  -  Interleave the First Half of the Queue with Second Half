import java.util.*;

class Solution {
    public void rearrangeQueue(Queue<Integer> q) {
        int n = q.size();
        if (n <= 2) return; // already interleaved

        int half = n / 2;
        Queue<Integer> firstHalf = new LinkedList<>();

        // Step 1: Move first half elements to firstHalf queue
        for (int i = 0; i < half; i++) {
            firstHalf.add(q.poll());
        }

        // Step 2: Interleave firstHalf and remaining elements in q
        while (!firstHalf.isEmpty()) {
            q.add(firstHalf.poll()); // element from first half
            q.add(q.poll());         // element from second half
        }
    }
}
