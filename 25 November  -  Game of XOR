class Solution {
    public int subarrayXor(int[] arr) {
        int n = arr.length;
        int result = 0;

        for (int i = 0; i < n; i++) {
            long freq = (long)(i + 1) * (n - i); // use long to avoid overflow
            if ((freq & 1L) == 1L) {
                result ^= arr[i];
            }
        }

        return result;
    }
}
